        "#@title ## üîß **Installation Settings**\n",
        "#@markdown ### Configure your Advanced RVC setup\n",
        "\n",
        "#@markdown Choose Python version:\n",
        "python_version = \"3.10\" #@param [\"3.10\", \"3.11\"]\n",
        "\n",
        "#@markdown Install Vietnamese language pack:\n",
        "install_vietnamese = True #@param {type:\"boolean\"}\n",
        "\n",
        "#@markdown Install additional voice models:\n",
        "install_models = False #@param {type:\"boolean\"}\n",
        "\n",
        "#@markdown Install high-quality audio models (requires more storage):\n",
        "high_quality = False #@param {type:\"boolean\"}\n",
        "\n",
        "#@markdown Enable advanced F0 methods (RMVPE, CREPE):\n",
        "advanced_f0 = True #@param {type:\"boolean\"}\n",
        "\n",
        "#@markdown Install music separation models:\n",
        "music_separation = True #@param {type:\"boolean\"}\n",
        "\n",
        "#@markdown Enable debug mode:\n",
        "debug_mode = False #@param {type:\"boolean\"}\n",
        "\n",
        "#@markdown ---\n",
        "#@markdown <br> *Run this cell to install Vietnamese-RVC & Applio integration.*\n",
        "\n",
        "import os\n",
        "import sys\n",
        "import subprocess\n",
        "import time\n",
        "import shutil\n",
        "from pathlib import Path\n",
        "from IPython.display import clear_output, display, HTML\n",
        "\n",
        "def install_vietnamese_rvc():\n",
        "    display(HTML(\"<h3>üáªüá≥üöÄ Installing Vietnamese-RVC & Applio Integration...</h3>\"))\n",
        "    \n",
        "    # 1. System Dependencies\n",
        "    print(\"üì¶ Installing system dependencies...\")\n",
        "    subprocess.run([\n",
        "        \"apt-get\", \"update\", \"-qq\"\n",
        "    ], capture_output=True)\n",
        "    \n",
        "    subprocess.run([\n",
        "        \"apt-get\", \"install\", \"-y\", \"-qq\", \n",
        "        \"ffmpeg\", \"git\", \"curl\", \"wget\", \"unzip\", \"aria2\", \"python3-dev\", \"build-essential\"\n",
        "    ], capture_output=True)\n",
        "    \n",
        "    # 2. Clone Repository\n",
        "    if not os.path.exists(\"Advanced-RVC-Inference\"):\n",
        "        print(\"üì• Cloning Vietnamese-RVC Enhanced Repository...\")\n",
        "        subprocess.run([\n",
        "            \"git\", \"clone\", \"https://github.com/ArkanDash/Advanced-RVC-Inference.git\"\n",
        "        ], check=True)\n",
        "    \n",
        "    os.chdir(\"Advanced-RVC-Inference\")\n",
        "    print(f\"üìÅ Changed to directory: {os.getcwd()}\")\n",
        "    \n",
        "    # 3. Python Dependencies\n",
        "    print(\"üêç Installing Python dependencies...\")\n",
        "    \n",
        "    # Core requirements\n",
        "    requirements = [\n",
        "        \"torch\", \"torchvision\", \"torchaudio\",\n",
        "        \"gradio==4.44.0\", \n",
        "        \"numpy\", \"scipy\", \"librosa\", \"soundfile\",\n",
        "        \"transformers\", \"accelerate\", \"onnx\", \"onnxruntime\",\n",
        "        \"faiss-gpu\", \"fairseq\", \"aubio\", \"praat-parselmouth\",\n",
        "        \"noisereduce\", \"pedalboard\", \"resampy\",\n",
        "        \"huggingface-hub\", \"gdown\", \"h5py\", \"lxml\",\n",
        "        \"sklearn\", \"matplotlib\", \"seaborn\", \"pandas\"\n",
        "    ]\n",
        "    \n",
        "    for req in requirements:\n",
        "        if not debug_mode:\n",
        "            subprocess.run([sys.executable, \"-m\", \"pip\", \"install\", req, \"-q\"], \n",
        "                         stdout=subprocess.DEVNULL, stderr=subprocess.DEVNULL)\n",
        "        else:\n",
        "            subprocess.run([sys.executable, \"-m\", \"pip\", \"install\", req], check=True)\n",
        "    \n",
        "    # 4. Download Vietnamese-RVC Models\n",
        "    print(\"üì• Downloading Vietnamese-RVC base models...\")\n",
        "    \n",
        "    # Create directories\n",
        "    os.makedirs(\"weights/hubert\", exist_ok=True)\n",
        "    os.makedirs(\"weights/pretrained\", exist_ok=True)\n",
        "    os.makedirs(\"weights/rmvpe\", exist_ok=True)\n",
        "    os.makedirs(\"assets/models/embedders\", exist_ok=True)\n",
        "    os.makedirs(\"assets/models/predictors\", exist_ok=True)\n",
        "    \n",
        "    # Base models for Vietnamese-RVC\n",
        "    base_models = {\n",
        "        \"weights/hubert/hubert_base.pt\": \"https://huggingface.co/lj1995/VoiceConversionWebUI/resolve/main/hubert_base.pt\",\n",
        "        \"weights/rmvpe/rmvpe.pt\": \"https://huggingface.co/lj1995/VoiceConversionWebUI/resolve/main/rmvpe.pt\",\n",
        "        \"weights/pretrained/pretrained_G40.pth\": \"https://huggingface.co/lj1995/VoiceConversionWebUI/resolve/main/pretrained_G40.pth\",\n",
        "        \"weights/pretrained/pretrained_D40.pth\": \"https://huggingface.co/lj1995/VoiceConversionWebUI/resolve/main/pretrained_D40.pth\"\n",
        "    }\n",
        "    \n",
        "    for file_path, url in base_models.items():\n",
        "        if not os.path.exists(file_path):\n",
        "            print(f\"  Downloading {file_path}...\")\n",
        "            subprocess.run([\n",
        "                \"aria2c\", \"--console-log-level=error\", \"-c\", \"-x\", \"16\", \"-s\", \"16\", \"-k\", \"1M\",\n",
        "                \"-d\", os.path.dirname(file_path), \"-o\", os.path.basename(file_path), url\n",
        "            ], check=True)\n",
        "    \n",
        "    # 5. Vietnamese Language Support\n",
        "    if install_vietnamese:\n",
        "        print(\"üáªüá≥ Setting up Vietnamese language support...\")\n",
        "        os.makedirs(\"assets/i18n/languages\", exist_ok=True)\n",
        "        # Vietnamese locale files would be placed here\n",
        "    \n",
        "    # 6. Advanced F0 Models\n",
        "    if advanced_f0:\n",
        "        print(\"üéµ Installing advanced F0 extraction models...\")\n",
        "        \n",
        "        # CREPE model\n",
        "        subprocess.run([\n",
        "            \"aria2c\", \"--console-log-level=error\", \"-c\", \"-x\", \"8\", \"-s\", \"8\",\n",
        "            \"-d\", \"weights/pretrained\", \"-o\", \"crepe_lite.pth\",\n",
        "            \"https://github.com/marl/crepe/releases/download/v1.0.1/crepe_lite.pth\"\n",
        "        ], check=True)\n",
        "    \n",
        "    # 7. Music Separation Models\n",
        "    if music_separation:\n",
        "        print(\"üéº Setting up music separation models...\")\n",
        "        os.makedirs(\"assets/models/uvr5\", exist_ok=True)\n",
        "        \n",
        "        mdx_models = {\n",
        "            \"UVR-MDX-NET_Main_390.onnx\": \"https://github.com/CNChTu/UVR-MDX-NET-Models/releases/download/Model/UVR-MDX-NET_Main_390.onnx\"\n",
        "        }\n",
        "        \n",
        "        for model_name, url in mdx_models.items():\n",
        "            model_path = f\"assets/models/uvr5/{model_name}\"\n",
        "            if not os.path.exists(model_path):\n",
        "                subprocess.run([\n",
        "                    \"aria2c\", \"--console-log-level=error\", \"-c\", \"-x\", \"8\", \"-s\", \"8\",\n",
        "                    \"-d\", \"assets/models/uvr5\", \"-o\", model_name, url\n",
        "                ], check=True)\n",
        "    \n",
        "    # 8. High Quality Models\n",
        "    if high_quality:\n",
        "        print(\"üî• Downloading high-quality models...\")\n",
        "        # Add high-quality model downloads here\n",
        "        pass\n",
        "    \n",
        "    # 9. Final Setup\n",
        "    print(\"‚öôÔ∏è  Finalizing setup...\")\n",
        "    \n",
        "    # Create essential directories\n",
        "    essential_dirs = [\n",
        "        \"weights\", \"indexes\", \"logs\", \"cache\", \"temp\", \n",
        "        \"audio_files\", \"outputs\", \"audios\", \"dataset\"\n",
        "    ]\n",
        "    \n",
        "    for dir_name in essential_dirs:\n",
        "        os.makedirs(dir_name, exist_ok=True)\n",
        "    \n",
        "    # Set permissions\n",
        "    os.chmod(\".\", 0o755)\n",
        "    \n",
        "    clear_output()\n",
        "    display(HTML(\"\"\"\n",
        "        <h3>‚úÖ Vietnamese-RVC & Applio Integration Complete!</h3>\n",
        "        <br>\n",
        "        <p><strong>üéØ Ready Features:</strong></p>\n",
        "        <ul>\n",
        "            <li>üáªüá≥ Vietnamese language support</li>\n",
        "            <li>üé§ Professional voice conversion</li>\n",
        "            <li>üéµ Music separation & audio effects</li>\n",
        "            <li>üöÄ Real-time conversion</li>\n",
        "            <li>üéöÔ∏è Advanced F0 extraction methods</li>\n",
        "            <li>üí¨ Text-to-speech integration</li>\n",
        "        </ul>\n",
        "        <br>\n",
        "        <p><em>Proceed to the next cell to launch the Advanced UI!</em></p>\n",
        "    \"\"\"))\n",
        "\n",
        "install_vietnamese_rvc()"
      ]
