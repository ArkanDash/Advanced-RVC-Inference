        "#@title ## üìÅ Upload Models\n",
        "#@markdown Use this to upload your RVC models to the logs directory\n",
        "from google.colab import files\n",
        "import os\n",
        "import shutil\n",
        "\n",
        "def upload_models():\n",
        "    print(\"üìÇ Uploading models to /content/Advanced-RVC-Inference/logs/\")\n",
        "    print(\"Please select your .pth model file and .index file\")\n",
        "    \n",
        "    # Upload files\n",
        "    uploaded = files.upload()\n",
        "    \n",
        "    # Process uploads\n",
        "    for filename in uploaded.keys():\n",
        "        filepath = f\"/content/{filename}\"\n",
        "        \n",
        "        # Create model directory\n",
        "        if filename.endswith('.pth'):\n",
        "            model_name = filename.replace('.pth', '')\n",
        "            model_dir = f\"/content/Advanced-RVC-Inference/logs/{model_name}\"\n",
        "            os.makedirs(model_dir, exist_ok=True)\n",
        "            \n",
        "            # Move model file\n",
        "            dest_path = f\"{model_dir}/{filename}\"\n",
        "            shutil.move(filepath, dest_path)\n",
        "            print(f\"‚úÖ Model {filename} uploaded to {dest_path}\")\n",
        "        \n",
        "        elif filename.endswith('.index'):\n",
        "            # Try to find corresponding model directory\n",
        "            model_name = filename.replace('.index', '').split('_')[0]\n",
        "            model_dir = f\"/content/Advanced-RVC-Inference/logs/{model_name}\"\n",
        "            os.makedirs(model_dir, exist_ok=True)\n",
        "            \n",
        "            # Move index file\n",
        "            dest_path = f\"{model_dir}/{filename}\"\n",
